<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.google.code.androidspycam"
      android:versionCode="1"
      android:versionName="0.0.1">
     <uses-permission android:name="android.permission.INTERNET" />
      
    <application android:icon="@drawable/camera" android:label="@string/app_name">
        <activity android:name=".Spycam"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

	<activity android:name=".PreferenceActivity" android:label="@string/PreferenceActivity">
			
	</activity>

    </application>
